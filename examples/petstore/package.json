{
  "name": "petstore",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "ts-node ./node_modules/moleculer/bin/moleculer-runner.js --config moleculer.config.ts --hot --repl services/**/*.service.ts",
    "start": "npm run build && moleculer-runner --config dist/moleculer.config.js --repl dist/services/**/*.service.js"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.11",
    "moleculer-repl": "^0.7.4",
    "openapi-types": "^12.1.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@spailybot/moleculer-auto-openapi": "^1.3.1",
    "fastest-validator": "^1.18.0",
    "jose": "^5.2.4",
    "moleculer": "^0.14.33",
    "moleculer-web": "^0.10.7",
    "swagger-ui-dist": "^5.17.7"
  },
  "engines": {
    "node": ">= 16.x.x"
  }
}
